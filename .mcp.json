{
  "mcpServers": {
    "flux-operator-mcp": {
      "command": "/usr/local/bin/flux-operator-mcp",
      "args": [
        "serve"
      ],
      "env": {
        "KUBECONFIG": "/home/smana/.kube/config"
      }
    },
    "victorialogs": {
      "command": "/usr/local/bin/mcp-victorialogs",
      "env": {
        "VL_INSTANCE_ENTRYPOINT": "https://vl.priv.cloud.ogenki.io"
      }
    },
    "victoriametrics": {
      "command": "/usr/local/bin/mcp-victoriametrics",
      "env": {
        "VM_INSTANCE_ENTRYPOINT": "https://vm.priv.cloud.ogenki.io",
        "VM_INSTANCE_TYPE": "single"
      }
    },
    "turbot-steampipe": {
      "command": "npx",
      "args": [
        "-y",
        "@turbot/steampipe-mcp"
      ],
      "env": {
        "STEAMPIPE_MCP_WORKSPACE_DATABASE": "${STEAMPIPE_DATABASE_URL}"
      }
    },
    "chrome-devtools": {
      "command": "npx",
      "args": [
        "-y",
        "chrome-devtools-mcp@latest",
        "--executablePath",
        "/usr/bin/chromium"
      ]
    }
  }
}
